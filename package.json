{
  "name": "RNSK",
  "version": "1.1.1",
  "description": "ðŸš€ Get started with React Native and Redux with no effort!",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlesMangwa/react-native-simple-kit"
  },
  "author": "Charles MANGWA <charles.mangwa@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CharlesMangwa/react-native-simple-kit/issues"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "cache-clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn autoclean --force",
    "lint": "eslint . --fix && git add .",
    "flow": "flow --show-all-errors",
    "jest": "jest --verbose",
    "test": "jest --verbose --coverage && cat ./__tests__/__coverage__/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prettier": "prettier --config .prettierrc --parser flow --write \"*.js\"",
    "precommit": "lint-staged && yarn lint && yarn flow && yarn jest",
    "postcommit": "git update-index --again"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn prettier",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "flow",
      "react-native"
    ],
    "env": {
      "production": {
        "plugins": [
          "minify-dead-code-elimination",
          "transform-inline-consecutive-adds",
          "transform-remove-console",
          "transform-react-remove-prop-types",
          "transform-flow-strip-types"
        ]
      }
    }
  },
  "engines": {
    "node": ">=8.3",
    "npm": ">=5"
  },
  "rnpm": {
    "assets": [
      "src/shared/assets/"
    ]
  },
  "jest": {
    "coverageDirectory": "./__tests__/__coverage__",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub"
    },
    "preset": "react-native",
    "rootDir": ".",
    "testRegex": ".*.spec.js$",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native)/.+"
    ]
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^1.1.0",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client-preset": "^1.0.8",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.0",
    "apollo-link-state": "^0.4.1",
    "apollo-link-ws": "^1.0.8",
    "apollo-utilities": "1.0.20",
    "graphql": "^14.0.0",
    "graphql-anywhere": "^4.1.18",
    "graphql-tag": "^2.9.2",
    "lodash.merge": "^4.6.1",
    "react": "16.4.2",
    "react-apollo": "^2.1.11",
    "react-native": "0.55.4",
    "react-native-modal": "6.5.0",
    "react-native-svg": "^6.5.2",
    "react-navigation": "^2.12.1",
    "react-redux": "^5.0.6",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "subscriptions-transport-ws": "^0.9.14"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-plugin-minify-dead-code-elimination": "^0.4.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "babel-plugin-transform-remove-console": "^6.8.5",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "^4.0.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "0.80.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "react-test-renderer": "16.4.2"
  }
}
